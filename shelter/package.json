{
  "name": "shelter",
  "scripts": {
    "clear": "rd /s /q dist",
    "develop": "set NODE_ENV=development&&webpack",
    "build": "npm run clear && set NODE_ENV=production&&webpack",
    "start": "set NODE_ENV=development&&webpack serve",
    "deploy": "npm run build && npx gh-pages -d dist -e shelter"
  },
  "devDependencies": {
    "css-loader": "^6.7.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^8.1.0",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.1",
    "sass-resources-loader": "^2.2.5",
    "style-loader": "^3.3.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  }
}
